public function WeaponType.toObjectString() returns string
	switch this
		case Normal
			return "normal"
		case Instant
			return "instant"
		case Artillery
			return "artillery"
		case ArtilleryLine
			return "aline"
		case Missile
			return "missile"
		case MissileSplash
			return "msplash"
		case MissileBounce
			return "mbounce"
		case MissileLine
			return "mline"
		default
			return "_"


